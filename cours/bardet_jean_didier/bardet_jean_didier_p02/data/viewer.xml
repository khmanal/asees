			<playbackActions></playbackActions>
			<slideTitle>@Objectifs pédagogiques du cours@</slideTitle>
			<notes isHTML="true">&lt;TextFlow xmlns = "http://ns.adobe.com/textLayout/2008" fontFamily = "Verdana" fontSize = "9"&gt;&lt;p&gt;&lt;span &gt;Ainsi,&lt;/span&gt;&lt;span &gt; les objectifs de ce cours son de d&lt;/span&gt;&lt;span &gt;écrire le phénomène&lt;/span&gt;&lt;span &gt; et d’étudier les conséquences pour les patients et la collectivité.&lt;/span&gt;&lt;span &gt;&lt;br/&gt;&lt;/span&gt;&lt;/p&gt;&lt;/TextFlow&gt;</notes>
			<slideThumbnail url="thumb.swf" frame="2"/>
