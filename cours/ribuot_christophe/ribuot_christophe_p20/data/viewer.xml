			<audios id="3" duration=" 34970">
				<media url="a24x3.mp3" startMsecs="0" duration="34970" startAtMSecs="0"/>
			</audios>
			<playbackActions></playbackActions>
			<slideTitle>@Les récepteurs à activité enzymatique@</slideTitle>
			<notes isHTML="true">&lt;TextFlow xmlns = "http://ns.adobe.com/textLayout/2008" fontFamily = "Verdana" fontSize = "9"&gt;&lt;p&gt;&lt;span &gt;Voici trois exemples de récepteurs à activité enzymatique : des Rx à activité tyrosine kinase comme les récepteurs à l’insuline que nos allons décrire pour illustrer cette dernière classe de récepteurs.&lt;/span&gt;&lt;span &gt;&lt;br/&gt;&lt;/span&gt;&lt;span &gt;Nous trouvons aussi des récepteurs à activité guanylyl-cyclase comme les récepteurs des facteurs natriuétiques. Nous parlons de ces facteurs dans le cours de Physiologie cardiovasculaire.&lt;/span&gt;&lt;span &gt;&lt;br/&gt;&lt;/span&gt;&lt;span &gt;Nous trouvons enfin les récepteurs à activité sérine/thréonine kinase.&lt;/span&gt;&lt;span &gt;&lt;br/&gt;&lt;/span&gt;&lt;/p&gt;&lt;/TextFlow&gt;</notes>
			<slideThumbnail url="thumb.swf" frame="3"/>
			<slideSpeaker index="0"/>
		</slide>
		<slide id="453" showInTOC="" inSequence="" frameRate="30" totalFrames="30" usePptxSlideDur="0" lockInteractionSlide="false" isHiddenInteractionSl="false" isWrongInteractionSl="false" swfConvertorEngine="Nova" slideDuration="94.72" advance="auto" blockMovement="false" hideplaybar="false" type="normal">
			<content url="Slide3.swf" id="453"/>
			<backdrop displayBgSp="true"/>
			<master id="2" frameNum="0"/>
			<layout id="3" frameNum="0"/>
			<slidexml><AS3Class className="TL_8d5e612de30c4f67ab60022f0263016fyxy" classType="PptTmline_xyxyxyxyxyxyxyxyxyxyxyxyxyxy" frameRate="30" normalSlide="1"><OnLoadCallBacks/><Audios/></AS3Class></slidexml>
