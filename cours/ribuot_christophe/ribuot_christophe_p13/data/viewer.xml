			<layout id="3" frameNum="0"/>
			<slidexml><AS3Class className="TL_7d228893d74649afb33325b8bdeff411yxy" classType="PptTmline_xyxyxyxyxyxyxyxyxyxyxyxyxyxy" frameRate="30" normalSlide="1"><OnLoadCallBacks/><Audios/></AS3Class></slidexml>
			<clickAnimations></clickAnimations>
			<webObjectContents/>
			<flvContents/>
			<audios id="5" duration=" 26560">
				<media url="a24x5.mp3" startMsecs="0" duration="26560" startAtMSecs="0"/>
			</audios>
			<playbackActions></playbackActions>
			<slideTitle>@La Pharmacologie en 2005 :@</slideTitle>
			<notes isHTML="true">&lt;TextFlow xmlns = "http://ns.adobe.com/textLayout/2008" fontFamily = "Verdana" fontSize = "9"&gt;&lt;p&gt;&lt;span &gt;30 ans plus tard, un certain nombre d’acteurs ont été décrits intervenant dans les voies de transduction du signal acétylcholine donne contraction.&lt;/span&gt;&lt;span &gt;&lt;br/&gt;&lt;/span&gt;&lt;span &gt;Nous allons volontairement dans le suite de ce cours nous positionner entre ces deux visions et nous efforcer de faire ni trop simple ni trop complexe.&lt;/span&gt;&lt;span &gt;&lt;br/&gt;&lt;/span&gt;&lt;/p&gt;&lt;/TextFlow&gt;</notes>
			<slideThumbnail url="thumb.swf" frame="5"/>
			<slideSpeaker index="0"/>
