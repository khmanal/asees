			<audios id="3" duration=" 47120">
				<media url="a24x3.mp3" startMsecs="0" duration="47120" startAtMSecs="0"/>
			</audios>
			<playbackActions></playbackActions>
			<slideTitle>@Les RCPG@</slideTitle>
			<notes isHTML="true">&lt;TextFlow xmlns = "http://ns.adobe.com/textLayout/2008" fontFamily = "Verdana" fontSize = "9"&gt;&lt;p&gt;&lt;span &gt;Les estimations actuelles du nombre de RCPG se situent autour de 800 récepteurs codés par des gènes distincts.&lt;/span&gt;&lt;span &gt;&lt;br/&gt;&lt;/span&gt;&lt;span &gt;La moitié sont activés par des stimuli sensoriels exogènes : photons, agents gustatifs ou olfactifs.&lt;/span&gt;&lt;span &gt;&lt;br/&gt;&lt;/span&gt;&lt;span &gt;L’autre moitié serait stimulée par des ligands endogènes.&lt;/span&gt;&lt;span &gt;&lt;br/&gt;&lt;/span&gt;&lt;span &gt;Il faut notre qu’un même ligand peut simuler plusieurs récepteurs qui lui sont propres.&lt;/span&gt;&lt;span &gt;&lt;br/&gt;&lt;/span&gt;&lt;/p&gt;&lt;/TextFlow&gt;</notes>
			<slideThumbnail url="thumb.swf" frame="3"/>
			<slideSpeaker index="0"/>
		</slide>
